/* logo */
div#logo h1
{
    margin: 0;
}
div#logo h1 a, div#logo h1 a:hover, div#logo h1 a:visited
{
    background: url("../images/logo/logo.png") no-repeat scroll center transparent;
    color:#fff;
	text-decoration:none;	
	text-indent:-9999px;
	width:428px;
	padding:25px 0;
	line-height:33px;
	display:block;
}

